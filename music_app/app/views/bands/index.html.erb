<section class="band-index">
  <h1> All of Our Bands! </h1>


  <section class="band-list">
    <ul>
    <% @bands.each do |band| %>
      <li>
        <a href="<%=band_url(band)%>"><%= band.name %></a>
      </li>
    <% end %>
  </ul>
  </section>

  <a href="<%= new_band_url %>">Add a Band!</a>

</section>
